{
  "name": "betterphotos",
  "version": "1.0.0",
  "description": "responsive ui built using flex box grid and scss",
  "main": "index.js",
  "scripts": {
    "compile": "sass --watch --no-source-map src/main.scss src/styles.css",
    "build:compile": "sass --no-source-map src/main.scss build/compiled.css",
    "build:prefixes": "postcss build/compiled.css --use autoprefixer --output build/prefixed.css",
    "build:optimize": "csso build/prefixed.css --output build/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dseals82/BetterPhotos.git"
  },
  "keywords": [
    "sass",
    "flexbox",
    "grid",
    "css"
  ],
  "author": "Darius seals",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dseals82/BetterPhotos/issues"
  },
  "homepage": "https://github.com/Dseals82/BetterPhotos#readme",
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "csso-cli": "^3.0.0",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "sass": "^1.51.0"
  }
}
